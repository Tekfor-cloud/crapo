<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >
    
 		<!-- All users should be able to read data --> 
        
        <record id="access_workflow_state_all" model="ir.model.access">
            <field name="name">access.workflow.state.readall</field>
        	<field name="model_id" ref="model_workflow_state"/>
        	<field name="group_id" eval="ref('base.group_user')"/>
        	<field name="perm_read">1</field>
        	<field name="perm_write">0</field>
        	<field name="perm_create">0</field>
        	<field name="perm_unlink">0</field>
        </record>
        
        <record id="access_workflow_transition_all" model="ir.model.access">
            <field name="name">access.workflow.transition.readall</field>
        	<field name="model_id" ref="model_workflow_transition"/>
        	<field name="group_id" eval="ref('base.group_user')"/>
        	<field name="perm_read">1</field>
        	<field name="perm_write">0</field>
        	<field name="perm_create">0</field>
        	<field name="perm_unlink">0</field>
        </record>
         
    </data>
</odoo>