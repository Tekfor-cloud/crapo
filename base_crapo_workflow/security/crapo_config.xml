<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<!-- All users should be able to read data -->

		<record id="access_crapo_config_admin" model="ir.model.access">
			<field name="name">access.crapo.config.admin</field>
			<field name="model_id" ref="model_crapo_config_settings" />
			<field name="group_id" eval="ref('group_crapo_admin')" />
